.requests(ng-show='storage.workspace.viewer==\'requests\'')
    .row
        h1 Requests
        .item(ng-repeat='item in viewers.requests',ng-class='item.status')
            .expand: a.fa.fa-caret-square-o-right
            .date {{item.ts}}
            .method {{item.method}}
            .url {{item.request.url}}
            .status {{item.response.status}}
            .clearfix
    .row

